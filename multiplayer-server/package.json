{
  "name": "paper-airplane-multiplayer-server",
  "version": "1.0.0",
  "description": "WebSocket server for Paper Airplane multiplayer game",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest tests/unit/",
